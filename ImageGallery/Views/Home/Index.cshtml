            <li><span>Category</span><div>Landscape</div></li>
            <li><span>Copyright</span><div>Landscape</div></li>
            <li><span>Avatar</span><div>Landscape</div></li>
        </ul>
    </div>
    <!-- Swiper JS -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/swiper")
    @Scripts.Render("~/bundles/main")


</body>
</html>
